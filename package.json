{
  "name": "p-stack-be",
  "version": "0.0.1",
  "description": "",
  "author": "Phong Thieu",
  "license": "ISC",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && node esbuild.mjs",
    "serve": "node dist/app.js",
    "format": "pnpm run lint --fix & prettier --write 'src/**/*.{js,ts}'",
    "lint:fix": "eslint src --ext .ts --fix",
    "lint": "eslint src --ext .ts",
    "migrate": "tsx src/db/migrate.ts",
    "migrate:create": "drizzle-kit generate:pg --config=drizzle.config.ts --custom",
    "migrate:generate": "drizzle-kit generate:pg --config=drizzle.config.ts",
    "migrate:drop": "drizzle-kit drop --config=drizzle.config.ts",
    "db:check": "drizzle-kit check:pg --config=drizzle.config.ts",
    "db:push": "drizzle-kit push:pg --config=drizzle.config.ts"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.10",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "drizzle-kit": "^0.20.13",
    "esbuild": "^0.20.0",
    "esbuild-plugin-copy": "^2.1.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@aws-sdk/client-ssm": "^3.501.0",
    "axios": "^1.6.7",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "drizzle-orm": "^0.29.3",
    "express": "^4.18.2",
    "express-promise-router": "^4.1.1",
    "postgres": "^3.4.3"
  }
}
